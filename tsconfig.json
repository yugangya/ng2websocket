{
  "compilerOptions": {
    "module": "commonjs",
    "target": "es5",
    "declaration": true,
    "sourceMap": true,
    "strictNullChecks": true,
    "noFallthroughCasesInSwitch": true,
    "noImplicitAny": true,
    "noImplicitReturns": true,
    "noImplicitThis": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "newLine": "lf"
  },
  "compileOnSave": false,
  "atom": {
    "rewriteTsconfig": false
  },
  "exclude": [
    "node_modules"
  ]
}
